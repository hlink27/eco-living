  <body>
    <%- include('includes/nav.ejs') %>
    <div class="index-menu">
      <% if(unidade.length > 0){ %>
        <% for(u of unidade){ %>
          <a href="/unidade/<%= u.id %>">
            <div class="item">
              <img src="<%= u.img %>">
            </div>
          </a>
        <% } %>
      <% } else { %>
        <h1 style="margin-left: 2rem;">:(</h1>
        <p></p><p></p>
        <p style="width: 200%;margin-left: 2rem;">Nenhuma unidade encontrada.</p>
      <% } %>
    </div>
  </body>
</html>